<selector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <item
        android:id="@+id/reserved"
        android:drawable="@drawable/ic_reserved"
        app:state_reserved="true" />

    <item
        android:id="@+id/wait_listed"
        android:drawable="@drawable/ic_waitlisted"
        app:state_wait_listed="true" />

    <item
        android:id="@+id/wait_list_available"
        android:drawable="@drawable/ic_waitlist_available"
        app:state_wait_list_available="true" />

    <item
        android:id="@+id/reservation_pending"
        android:drawable="@drawable/ic_reservation_pending"
        app:state_reservation_pending="true" />

    <item
        android:id="@+id/reservation_disabled"
        android:drawable="@drawable/ic_reservation_disabled"
        app:state_reservation_disabled="true" />

    <item
        android:id="@+id/reservable"
        android:drawable="@drawable/ic_reservable" />

</selector>
